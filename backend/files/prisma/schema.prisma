generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model User {
  id        String    @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  lastName  String
  username  String    @unique
  password  String 
  company   String
  email     String    @unique
  isActive  Boolean
  createdAt DateTime  @default(now())
  updatedAt DateTime  @default(now()) @updatedAt

  @@map("User")
}
